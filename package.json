{
  "name": "@leewinter/auto-table",
  "author": {
    "name": "Lee Winter",
    "email": "lee.winter@boxspark.co.uk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leewinter/auto-table"
  },
  "license": "MIT",
  "version": "0.3.1",
  "description": "Basic dynamic table",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./dist/style.css": "./dist/esm/assets/bundle-styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rollup -c",
    "lint": "eslint --ignore-path .gitignore --fix src",
    "format": "prettier --write src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "keywords": [
    "rollup",
    "javascript",
    "library",
    "esm"
  ],
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.21.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@storybook/addon-essentials": "^7.0.27",
    "@storybook/addon-interactions": "^7.0.27",
    "@storybook/addon-links": "^7.0.27",
    "@storybook/blocks": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@storybook/react-vite": "^7.0.27",
    "@storybook/testing-library": "^0.0.14-next.2",
    "eslint": "^8.39.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "rollup": "^3.23.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-import-css": "^3.3.1",
    "rollup-plugin-preserve-directives": "^0.2.0",
    "storybook": "^7.0.27",
    "rollup-plugin-peer-deps-external": "^2.2.4"
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": "^16.14.0 || ^17.0.0 || ^18.2.0",
    "react-dom": "^16.14.0 || ^17.0.0 || ^18.2.0"
  },
  "engines": {
    "node": ">=16.0.0 || >=18.0.0 || >=19.0.0 || >=20.0.0"
  },
  "lint-staged": {
    "*.js": "npm run lint",
    "*.{js,css,md*}": "npm run format"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
